    <!-- PAGE WRAP -->
    <div id="page-wrap">
        <!-- PRELOADER -->
        <div class="preloader"></div>
        <!-- END / PRELOADER -->
        <!-- HEADER PAGE -->


        <header id="header" class="header normal">
            <div class="container-fluid px-0" style="width: 100%">
                <div class="row px-0" style="width: 100%; margin: 0px; background-color: #fff">
                    <div class="col-xs-12">
                        <div class="header-cont clearfix">
                            <a href="@Url.Action("Index", "Home")" class="logo"> <img src="~/images/logo4.jpg" alt=""> </a>
                            <div class="header-info">
                                <div class="social-content clearfix">
                                    <div class="contact-det hidden-xs">
                                        <ul>
                                            <li>
                                                <a href="tel:+966001234567">+966 800 12 34 567 </a>/ 
                                            </li>
                                            <li>
                                                <a href="mailto:mail@cargomate.com">mail@cargomate.com</a>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="social-det">
                                        <ul class="hide">
                                            <li>
                                                <a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                                            </li>
                                            <li>
                                                <a href="#"><i class="fa fa-instagram" aria-hidden="true"></i></a>
                                            </li>
                                            <li>
                                                <a href="#"><i class="fa fa-google-plus" aria-hidden="true"></i></a>
                                            </li>
                                            <li>
                                                <a href="#"><i class="fa fa-youtube-play" aria-hidden="true"></i></a>
                                            </li>
                                            <li>
                                                <a href="#"><i class="fa fa-vk" aria-hidden="true"></i></a>
                                            </li>

                                        </ul>
                                        <div class="select-box  hidden-xs">
                                            <select class="select selectBox" style="display: none;">
                                                <option value="English">EN</option>
                                                <option value="French">FR</option>
                                                <option value="German">GR</option>
                                            </select><a class="selectBox select selectBox-dropdown" title="" tabindex="0" style="width: 40px; display: inline-block;"><span class="selectBox-label" style="width: 17px;">EN</span><span class="selectBox-arrow"></span></a>

                                        </div>
                                        <div class="login">
                                            <a href="@Url.Action("SignIn", "Home")" id="btnSignIn"><span>Login</span><i class="fa fa-lock" aria-hidden="true"></i></a>
                                            <a href="javascript:" class="hide"  id="btnSignOut"><span>LogOut</span><i class="fa fa-lock" aria-hidden="true"></i></a>
                                            <a href="@Url.Action("Register", "Customer")"><span>Become a member</span><i class="fa fa-user" aria-hidden="true"></i></a>
                                            <a href="@Url.Action("BecomeDriver", "Home")" class="btn btn-primary radius-1"><span>Become a driver</span><i class="fa fa-truck" aria-hidden="true"></i></a>
                                        </div>
                                    </div>
                                </div>

                                <nav class="navbar">

                                    <!-- Brand and toggle get grouped for better mobile display -->
                                    <div class="navbar-header">
                                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                                            <span class="sr-only">Toggle navigation</span>
                                            <span class="icon-bar"></span>
                                            <span class="icon-bar"></span>
                                            <span class="icon-bar"></span>
                                        </button>
                                    </div>

                                    <!-- Collect the nav links, forms, and other content for toggling -->
                                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                                        <ul class="nav navbar-nav navigation clearfix">

                                            <li class="active">
                                                <a href="#">RIDERS</a>
                                            </li>
                                            <li class="drop-down-parent">
                                                <a href="#">VEHICLES</a>
                                            </li>

                                            <li class="drop-down-parent">
                                                <a href="#">CITIES</a>
                                            </li>
                                            <li>
                                                <a href="#">ABOUT</a>
                                            </li>
                                            <li>
                                                <a href="#">CONTACT</a>
                                            </li>

                                        </ul>
                                        <div class="header-search">
                                            <i class="fa fa-search" aria-hidden="true"></i>
                                            <input type="text" class="search-bar" placeholder="Search here...">
                                        </div>
                                    </div><!-- /.navbar-collapse -->

                                </nav>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>

        <!-- END / HEADER PAGE -->
        @RenderBody()

        <!-- FOOTER PAGE -->
        <footer id="footer-page" class="py-0">
            <div class="container">
                <div class="copyright">
                    <p>©@DateTime.Now.Year Cargomate travel™ All rights reserved.</p>
                </div>
            </div>
        </footer>
        <!-- END / FOOTER PAGE -->


    </div>
    <!-- END / PAGE WRAP -->
